{
  "editor.tabSize": 2,
  "files.trimFinalNewlines": true,
  "files.insertFinalNewline": true,
  "editor.detectIndentation": false,
  "files.trimTrailingWhitespace": true,
  "search.exclude": {
    "**/yarn.lock": true,
    "**/package-lock.json": true,
    "**/npm-shrinkwrap.json": true
  },
  "files.exclude": {
    "**/*.class": true,
    "**/.project": true,
    "**/.DS_Store": true,
    "**/.settings": true,
    "**/.classpath": true,
    "**/.directory": true,
    "**/desktop.ini": true,
    "**/.factorypath": true,
    "**/node_modules/": true
  }
}
